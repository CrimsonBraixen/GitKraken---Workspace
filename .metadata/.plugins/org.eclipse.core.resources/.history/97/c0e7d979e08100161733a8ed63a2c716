package punto;

public interface Punto {
	
	public Punto clone(Punto punto);
	public double distanciaCon(Punto punto);
	public double distanciaAlOrigen();
	public void desplazamiento(double dx, double dy, double dz);
	public void desplazamiento(double dx, double dy);
	public boolean equals(Punto punto);
	public double getX();
	public double getY();
	public double getZ();
	public double setX(double x);
	public double setY(double y);
	public double setZ(double z);
}
