package conflictosEntreReinas;

public class Reina {

	private int fila;
	private int columna;
	
	public Reina(){
		fila=1;
		columna=1;
	}
	
	public Reina(int fila, int columna){
		this.fila = fila;
		this.columna = columna;
	}

	public int getFila() {
		return fila;
	}

	public void setFila(int fila) {
		this.fila = fila;
	}

	public int getColumna() {
		return columna;
	}

	public void setColumna(int columna) {
		this.columna = columna;
	}
	
}
