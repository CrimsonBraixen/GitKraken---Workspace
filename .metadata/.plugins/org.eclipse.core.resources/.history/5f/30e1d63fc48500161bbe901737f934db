package conflictosEntreReinas;

public class Reina {

	private int id;
	private int fila;
	private int columna;
	
	
	public Reina(int id, int fila, int columna){
		this.id = id;
		this.fila = fila;
		this.columna = columna;
	}

	public int getFila() {
		return fila;
	}

	public void setFila(int fila) {
		this.fila = fila;
	}

	public int getColumna() {
		return columna;
	}

	public void setColumna(int columna) {
		this.columna = columna;
	}
	
}
